<div id="targeting">
<div class="nested-fields">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title">Add New Targeting</h3>
		</div>
		<div class="panel-body">
			
		
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 style="display:inline" class="panel-title">Places</h3>
				</div>
				<div class="panel-body">
					<div id="places">

						<% f.fields_for :places do |place| %>
							<%= render 'places_fields', f: place %>
						<% end %>
						<div class="links">
							<%= link_to_add_association 'Add Place', f, :places, :class => 'btn btn-success' %>
						</div>
					</div>
				</div>
			</div>




			<div class="field">
				<%= f.label :gender %>
				<%= f.text_field :gender, class: 'form-control'%><br>
			</div>
			<%= link_to_remove_association "Remove Targeting", f, :class => 'btn btn-danger'%>		

		</div>
	</div>
	</div>
</div>