<div id="targeting">
<div class="nested-fields">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title">Add New Targeting</h3>
		</div>
		<div class="panel-body">
			
		
			<div class="field">
				
					<%= f.label :places %>
					<%= f.text_field :places, class: 'form-control'%>
			</div>
			<div class="field">
				<%= f.label :gender %>
				<%= f.text_field :gender, class: 'form-control'%><br>
			</div>
			<%= link_to_remove_association "Remove Targeting", f, :class => 'btn btn-danger'%>		

		</div>
	</div>
	</div>
</div>