<div id="creative">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title">Creative</h3>
		</div>
		<div class="panel-body">
			
		<div class="nested-fields">
			<div class="field">
				<%= f.label :bid %>
				<%= f.text_field :bid %>
			</div>
			<div class="field">
				<%= f.label :adtext %>
				<%= f.text_field :adtext %>
			</div>
		</div>

		<%= link_to_remove_association "Remove Creative", f, :class => 'btn btn-danger'%>

		</div>
	</div>
</div>